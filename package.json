{
  "name": "openator",
  "version": "v1.0.3",
  "description": "SORA AI-browsing agent that is capable of planning and executing actions formulated in natural language.",
  "author": "Kevin Piacentini <kevin@agentlabs.dev>",
  "private": false,
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/agentlabs-dev/openator",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentlabs-dev/openator.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node@*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "npx rollup -c",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@hyperbrowser/sdk": "^0.30.0",
    "@mozilla/readability": "^0.5.0",
    "class-validator": "^0.14.1",
    "dom-to-semantic-markdown": "^1.3.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "jsdom": "^26.0.0",
    "langchain": "^0.3.19",
    "playwright": "^1.50.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.0",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "semantic-release": "^24.2.3",
    "semantic-release-gitmoji": "^1.6.8",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
